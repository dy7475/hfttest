  	<select id="getBuildSupplyDemandComparisonForBuy" resultType="java.util.Map">
		select top 15 a.SOURCE_ID buildId,count(1) bulidCustCount
		from FUN_CUST_DEMAND_SPLIT_3 a
		join FUN_BUY_CUSTOMER_MIN b on b.BUY_CUST_ID=a.CASE_ID and a.CITY_ID = b.CITY_ID and b.CITY_ID = #{shardCityId}
		where a.CASE_TYPE = 3
		and a.CITY_ID = #{shardCityId}
		AND b.${param.serchRange}=#{param.serchRangeId}<!-- 搜索范围 -->
		<if test="param.statusType == 1">AND b.BUY_CUST_STATUS &lt;= 4</if><!-- 有效 -->
		AND b.COMP_ID = #{param.compId}
		group by a.SOURCE_ID
		order by count(1) desc
	</select>
	
	<select id="getBuildSupplyDemandComparisonForRent" resultType="java.util.Map">
		select top 15 a.SOURCE_ID buildId,count(1) bulidCustCount
		from FUN_CUST_DEMAND_SPLIT_3 a
		join FUN_RENT_CUSTOMER_MIN b on b.RENT_CUST_ID=a.CASE_ID and a.CITY_ID = b.CITY_ID and b.CITY_ID = #{shardCityId}
		where a.CASE_TYPE = 4
		AND b.${param.serchRange}=#{param.serchRangeId}<!-- 搜索范围 -->
		<if test="param.statusType == 1">AND b.RENT_CUST_STATUS &lt;= 4</if><!-- 有效 -->
		AND b.COMP_ID = #{param.compId} 
		and a.CITY_ID = #{shardCityId}
		group by a.SOURCE_ID
		order by count(1) desc
	</select>
