<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.myfun.repository.agencydb.dao.AgencyMessageFavMapper" >
  <resultMap id="BaseResultMap" type="com.myfun.repository.agencydb.po.AgencyMessageFav" >
    <!--@mbggenerated-->
    <result column="ARCHIVE_ID" property="archiveId" jdbcType="INTEGER" />
    <result column="USER_NO" property="userNo" jdbcType="VARCHAR" />
    <result column="MSG_ID" property="msgId" jdbcType="INTEGER" />
    <result column="MESS_TIME" property="messTime" jdbcType="TIMESTAMP" />
  </resultMap>
  <resultMap type="com.myfun.repository.agencydb.dto.AgencyMessageFavDto" id="extResultMap" extends="BaseResultMap">
  	<result column="CREATION_TIME" property="creationTime" jdbcType="TIMESTAMP" />
  	<result column="SPREAD_CONTENT" property="spreadContent" jdbcType="VARCHAR" />
  	<result column="BUILD_NAME" property="buildName" jdbcType="VARCHAR" />
  	<result column="IS_READ" property="isRead" jdbcType="VARCHAR" />
  	<result column="SPREAD_ID" property="spreadId" jdbcType="INTEGER" />
  	<result column="SPREAD_TITLE" property="spreadTitle" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.myfun.repository.agencydb.po.AgencyMessageFav" >
    <!--@mbggenerated-->
    insert into MESSAGE_FAV (ARCHIVE_ID, USER_NO, MSG_ID, 
      MESS_TIME)
    values (#{archiveId,jdbcType=INTEGER}, #{userNo,jdbcType=VARCHAR}, #{msgId,jdbcType=INTEGER}, 
      #{messTime,jdbcType=TIMESTAMP})
  </insert>
  <insert id="insertSelective" parameterType="com.myfun.repository.agencydb.po.AgencyMessageFav" >
    <!--@mbggenerated-->
    insert into MESSAGE_FAV
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="archiveId != null" >
        ARCHIVE_ID,
      </if>
      <if test="userNo != null" >
        USER_NO,
      </if>
      <if test="msgId != null" >
        MSG_ID,
      </if>
      <if test="messTime != null" >
        MESS_TIME,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="archiveId != null" >
        #{archiveId,jdbcType=INTEGER},
      </if>
      <if test="userNo != null" >
        #{userNo,jdbcType=VARCHAR},
      </if>
      <if test="msgId != null" >
        #{msgId,jdbcType=INTEGER},
      </if>
      <if test="messTime != null" >
        #{messTime,jdbcType=TIMESTAMP},
      </if>
    </trim>
  </insert>
  <select id="getDistMsgConnllectionList" parameterType="java.util.Map" resultMap="extResultMap">
  	SELECT sp.CREATION_TIME,sp.SPREAD_CONTENT,BI.BUILD_NAME,"1" as IS_READ,mf.MESS_TIME,mf.MSG_ID as SPREAD_ID,sp.SPREAD_TITLE
  	FROM MESSAGE_FAV mf
  	LEFT JOIN SPREAD sp ON mf.msg_id = sp.spread_id
  	LEFT JOIN BUILDING_INFO bi ON sp.build_id = bi.build_id
  	WHERE mf.ARCHIVE_ID = #{archiveId}
  </select>
  <select id="countByCondition" parameterType="java.util.Map" resultType="java.lang.Integer">
  	select count(1) from MESSAGE_FAV WHERE MSG_ID = #{spreadId} and ARCHIVE_ID = #{archiveId}
  </select>
  <delete id="deleteByCondition" parameterType="java.util.Map">
  	delete from MESSAGE_FAV where MSG_ID = #{spreadId} and ARCHIVE_ID = #{archiveId}
  </delete>
</mapper>