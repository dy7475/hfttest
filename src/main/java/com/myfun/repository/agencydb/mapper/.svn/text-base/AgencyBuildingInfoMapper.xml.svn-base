           A.UPDATE_TIME, A.BUILD_STATUS, A.BUILD_PROGRESS, A.PROJECT_SPEC, A.RIGHT_YEARS, A.BUILD_PERMISSION,
           A.SELL_DATE, A.FINISH_DATE, A.BUILD_FITMENT, A.CONSTRACT_AREA, A.SELL_ADDR, A.SELL_TELE, A.PRICE,
           A.PRICE_TEXT, A.PRICE_TOTAL, A.PHOTO_EXTERIOR, A.PHOTO_EFFECTS, A.PHOTO_MODEL, A.PHOTO_TRAFFIC,
           A.PHOTO_PLAN, A.PHOTO_SET, A.PHOTO_TEMPLET, A.NEW_FLAG, A.BUILD_USAGE, A.EXAM_PHOTO, A.SELL_TELE1,
           A.DEVLOPER_ID, A.SALE_LICENSE, A.DEVLOPER_RATE, A.LAND_USEAGE, A.LAND_USEYEARS, A.LAND_AREA,
           A.BUILD_DENSITY, A.DECORATION, A.STD_FITTING, A.PARK_RENT, A.PARK_SALE, A.COMP_INVEST, A.COMP_PROPER,
           A.COMP_DESILAND, A.COMP_DESIARCH, A.COMP_CONSTR, A.AGENCY_TYPE, A.AGENCY_STATUS, A.EDIT_FLAG,
           A.CRM_COMP_ID, A.CRM_USER_ID, A.DEVLOPER_CITY_ID, A.SEQ_NO, A.CONFIRM_NUM, A.DEAL_NUM, A.IS_TOP,
           A.SUPPLY_HEATING, A.WATER_SUPPLY, A.USEAGE_TYPE, A.CONTRACTOR, A.FLOOR_TEXT, A.ROOM_TEXT, A.BUILD_SOURCE,
           A.OPEN_DATE, A.DEVLOPER_NAME, A.HAOFANG_AMOUNT, A.ERP_COMP_NO, A.ERP_COMP_ID, A.SYSTEM_FLAG,
           A.ERP_BUILD_ID,A.PHONE_LIMIT_MODE, D.COMP_CNAME, B.ID, B.CONTRACT_NO,
  			B.REPEAT_DAY, B.BAND_REWARD_NUM, B.DEAL_REWARDS_NUM, B.DRAW AS DRAW, B.BAND_SAW,
  		    B.SOLE_SPECIAL, B.GROUP_SPECIAL, B.COMMISSION, B.BAND_REWARD, B.DEAL_REWARD,
  		    B.COOPERATE_START, B.COOPERATE_END, C.LOGIN_UID
  		FROM BUILDING_INFO A
  		LEFT JOIN COOPERATION_RULE B ON A.CITY_ID=B.CITY_ID AND A.BUILD_ID=B.BUILD_ID AND A.DEVLOPER_ID=B.DEVLOPER_ID
  		LEFT JOIN FUN_USERS C ON A.CITY_ID=C.CITY_ID AND A.BUILD_ID=C.BUILD_ID AND C.USER_POSITION='BUILD_MANAGER'
  		LEFT JOIN FUN_COMP_REAL D ON A.CITY_ID=D.CITY_ID AND A.DEVLOPER_ID=D.DEVLOPER_ID
  		WHERE A.BUILD_ID=#{buildId}
  </select>
  
  <select id="getBuildInfoList" parameterType="java.util.Map" resultMap="extResultMap">
  		select A.BUILD_ID, A.DEVLOPER_ID, A.BUILD_NAME, A.AGENCY_STATUS, A.PRICE_TEXT, C.COMP_NAME,C.COMP_CNAME, D.COMMISSION, D.ID,
  				A.BUILD_REGION, A.REG_NAME, A.SECTION_ID,A.BUILDING_ADMIN, SECTION_NAME,E.USER_NAME
		from BUILDING_INFO A LEFT JOIN FUN_COMP_REAL C ON A.DEVLOPER_ID=C.DEVLOPER_ID AND A.CITY_ID=C.CITY_ID AND A.ERP_COMP_ID=C.ERP_COMP_ID
		LEFT JOIN COOPERATION_RULE D ON A.DEVLOPER_ID=D.DEVLOPER_ID AND A.CITY_ID=D.CITY_ID AND A.BUILD_ID=D.BUILD_ID
		LEFT JOIN FUN_USERS E ON A.BUILDING_ADMIN=E.USER_ID
		<where>
			<if test="1==1">
				AND A.CITY_ID = #{cityId} and A.SYSTEM_FLAG=1 and A.ERP_COMP_ID = #{erpCompId}
			</if>
			<if test="null != buildId and '' != buildId">
				AND A.BUILD_ID in (#{buildId})
			</if>
			<if test="null != sectionId and '' != sectionId">
				AND A.SECTION_ID = #{sectionId}
			</if>
			<if test="null != agencyStatus and '' != agencyStatus">
				<choose>
					<when test="agencyStatus = 0">
						AND A.AGENCY_STATUS= #{agencyStatus}
					</when>
					<otherwise>
						AND A.AGENCY_STATUS >= #{agencyStatus}
					</otherwise>
				</choose>
			</if>
		</where>
  </select>
  <select id="selectMaxSeq" parameterType="java.lang.String" resultMap="BaseResultMap">
  	select top 1 SEQ_NO from BUILDING_INFO where CITY_ID=#{cityId} order by SEQ_NO desc
  </select>
  <update id="updateAddSeqNo">
  	update BUILDING_INFO 
  	set SEQ_NO=SEQ_NO+1 
  	<where>
  		<if test="null != cityId">
  			and CITY_ID=#{cityId}
  		</if>
  		<if test="null != seqNo">
  			<![CDATA[and SEQ_NO>=#{seqNo}	]]>
  		</if>
  		<if test="null != oldSeqNo">
  			<![CDATA[SEQ_NO<#{oldSeqNo}]]>
  		</if>
  	</where>
  
  </update>
  <update id="updateSubSeqNo">
  	update BUILDING_INFO 
  	set SEQ_NO=SEQ_NO-1 
  		<where>
  		<if test="null != cityId">
  			and CITY_ID=#{cityId}
  		</if>
  		<if test="null != seqNo">
  			<![CDATA[and SEQ_NO>#{seqNo}]]>
  		</if>
  		<if test="null != oldSeqNo">
  			<![CDATA[SEQ_NO<=#{oldSeqNo}]]>
  		</if>
  	</where>
  </update>
	
	<select id="getBuildInfoListOnIndex" resultMap="IndexResultMap">
		SELECT BI.PRICE_UNIT,BI.SEQ_NO, BI.TOTAL_PRICE_LOW,BI.TOTAL_PRICE_HIGH,BI.BUILD_TYPE,BI.DEVLOPER_NAME,
		BI.PHONE_LIMIT_MODE,BI.BUILD_SOURCE,BI.ERP_COMP_ID,BI.SYSTEM_FLAG,BI.AGENCY_STATUS,BI.CREATE_TIME,BI.IS_TOP,
		BI.UPDATE_TIME,BI.CITY_ID,BB.CITY_ID AS B_CITY_ID,BI.BUILD_ID,BI.SELL_TELE,BI.DEVLOPER_ID,BI.BUILD_STATUS,
		BI.PRICE,BI.PRICE_TEXT,BI.BUILD_NAME,BI.BUILD_ADDR,BI.PROJECT_SPEC,BI.CONFIRM_NUM,BI.DEAL_NUM,FCR.COMP_NAME,
		CR.GROUP_SPECIAL,CR.SOLE_SPECIAL, CR.COMMISSION,CR.BAND_REWARD,CR.DEAL_REWARD,CR.COOPERATE_START,
		CR.COOPERATE_END,
		(
			SELECT TOP 1 PHOTO_ADDR FROM BUILDING_PHOTO WHERE BI.BUILD_ID = BUILDING_PHOTO.BUILD_ID AND
			BI.CITY_ID = BUILDING_PHOTO.CITY_ID AND BUILDING_PHOTO.PHOTO_TYPE IN(1,2) AND EDIT_FLAG != 1 ORDER BY BUILDING_PHOTO.IS_FIRST DESC
		) AS PHOTO_ADDR,
		(SELECT COUNT(*) + BI.JOIN_COUNT_OFFSET FROM FUN_CUST FC WHERE FC.BUILD_ID = BI.BUILD_ID AND  FC.B_CITY_ID = BI.CITY_ID AND FC.CUST_STATUS >= 1) CUSTOMER_NUM,
		(SELECT COUNT(DISTINCT ARCHIVE_ID) + BI.INTENTION_COUNT_OFFSET FROM FUN_CUST FCU WHERE FCU.BUILD_ID = BI.BUILD_ID) ARCHIVE_NUM,
		(SELECT TOP 1 BA.ACT_CONTENT FROM BUILD_ACTIVITY BA WHERE BA.BUILD_ID = BI.BUILD_ID AND BA.BOUND = 2) ACT_CONTENT
		FROM BUILDING_INFO BI
		LEFT JOIN BUILDING_BOUND BB ON BI.BUILD_ID = BB.BUILD_ID
		LEFT JOIN BUILDING_COMP BC ON BI.BUILD_ID = BC.BUILD_ID
		LEFT JOIN COOPERATION_RULE CR ON BI.CITY_ID = CR.CITY_ID AND BI.BUILD_ID = CR.BUILD_ID AND CR.COOPERATE_END IS NOT NULL AND CR.COOPERATE_START IS NOT NULL
		LEFT JOIN FUN_COMP_REAL FCR ON BI.CITY_ID = FCR.CITY_ID AND BI.DEVLOPER_ID = FCR.DEVLOPER_ID
		WHERE
		AGENCY_STATUS != 0 AND BI.ERP_FLAG=1 AND BI.SYSTEM_FLAG != 2
		AND (
			BB.CITY_ID = #{cityId} OR BB.CITY_ID = 0
			OR ( BI.ERP_COMP_ID=#{compId} AND BI.CITY_ID=#{cityId})
			OR BC.COMP_NO=#{compNo}
		)
		<if test="platformType != null and platformType != ''">
			AND BI.SYSTEM_FLAG= #{platformType} AND BI.ERP_FLAG=1
		</if>
		<if test="buildRegion != null and buildRegion != ''">
			AND BI.BUILD_REGION= #{buildRegion}
		</if>
		<if test="sectionId != null and sectionId != ''">
			AND BI.SECTION_ID= #{sectionId}
		</if>
		<if test="buildIdArr != null">
			AND BI.BUILD_ID IN
			<foreach collection="buildIdArr" item="buildId" open="(" close=")" separator=",">
				#{buildId}
			</foreach>
		</if>
		ORDER BY
		<choose>
			<when test="orderBy == null or orderBy == ''">
				IS_TOP, SEQ_NO DESC,AGENCY_STATUS DESC,CREATE_TIME DESC
			</when>
			<when test="orderBy == 'new'">
				SEQ_NO DESC, CREATE_TIME DESC
			</when>
			<when test="orderBy == 'hot'">
				IS_TOP,SEQ_NO DESC, (CASE WHEN AGENCY_STATUS IN(2)THEN 1 ELSE 0 END ) DESC
			</when>
			<when test="orderBy == 'price'">
				PRICE DESC, SEQ_NO DESC
			</when>
			<otherwise>IS_TOP, SEQ_NO DESC,AGENCY_STATUS DESC,CREATE_TIME DESC</otherwise>
		</choose>
	</select>

  <select id="getBuildingInfoByUserId" resultType="java.lang.Integer">
    select
      top 1 BUILD_ID
    from BUILDING_INFO
    WHERE USER_ID = #{userId}
  </select>
	
</mapper>

