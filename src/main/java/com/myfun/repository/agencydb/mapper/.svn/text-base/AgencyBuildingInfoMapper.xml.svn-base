  <!-- ERP  -->
  <select id="getDistBuildingList" parameterType="java.util.Map" resultMap="extResultMap">
  	 SELECT BI.DEVLOPER_NAME, BI.PHONE_LIMIT_MODE, BI.BUILD_SOURCE, BI.ERP_COMP_ID, BI.SYSTEM_FLAG,
		BI.AGENCY_STATUS, BI.CREATE_TIME, BI.IS_TOP, BI.UPDATE_TIME, BI.CITY_ID, BB.CITY_ID AS B_CITY_ID, BI.BUILD_ID,
		BI.SELL_TELE, BI.DEVLOPER_ID, BI.BUILD_STATUS, BI.PRICE, BI.PRICE_TEXT, BI.BUILD_NAME, BI.BUILD_ADDR,
		BI.PROJECT_SPEC, BI.CONFIRM_NUM, BI.DEAL_NUM, FCR.COMP_NAME, CR.GROUP_SPECIAL, CR.SOLE_SPECIAL, CR.COMMISSION,
		CR.BAND_REWARD, CR.DEAL_REWARD, CR.COOPERATE_START, CR.COOPERATE_END,
		(SELECT TOP 1 PHOTO_ADDR FROM BUILDING_PHOTO WHERE BI.BUILD_ID = BUILDING_PHOTO.BUILD_ID AND BI.CITY_ID = BUILDING_PHOTO.CITY_ID AND BUILDING_PHOTO.PHOTO_TYPE IN (1, 2) AND EDIT_FLAG != 1 ORDER BY BUILDING_PHOTO.IS_FIRST DESC ) PHOTO_ADDR,
		(SELECT COUNT (*) FROM FUN_CUST fc WHERE fc.build_id = BI.BUILD_ID AND fc.B_CITY_ID = BI.CITY_ID AND fc.CUST_STATUS >= 1 ) CUSTOMER_NUM,
		(SELECT COUNT (DISTINCT archive_id) FROM fun_cust fcu WHERE fcu.build_id = bi.build_id ) ARCHIVE_NUM,
		(SELECT TOP 1 ba.ACT_CONTENT FROM BUILD_ACTIVITY ba WHERE ba.build_id = bi.build_id AND ba.BOUND = 2 ) ACT_CONTENT
		FROM BUILDING_INFO BI
		LEFT JOIN BUILDING_BOUND bb ON BI.build_id = bb.build_id
		LEFT JOIN BUILDING_COMP bc ON BI.build_id = bc.build_id
		LEFT JOIN COOPERATION_RULE cr ON BI.city_id = cr.city_id AND BI.build_id = cr.build_id AND cr.COOPERATE_END IS NOT NULL AND cr.COOPERATE_START IS NOT NULL
		LEFT JOIN FUN_COMP_REAL fcr ON bi.city_id = fcr.city_id AND bi.DEVLOPER_ID = fcr.DEVLOPER_ID
		<where>
			<if test="1==1">
				<![CDATA[and AGENCY_STATUS <> 0]]>
				AND ((BB.CITY_ID = #{CITY_ID} OR BB.CITY_ID = 0) OR ( bi.SYSTEM_FLAG = 1 AND ( bi.ERP_COMP_ID = #{COMP_ID} OR bc.COMP_NO = #{COMP_NO} )))
			</if>
			<if test="null != plateType and '' != plateType ">
				AND bi.SYSTEM_FLAG = #{plateType}
			</if>
		</where>
		<choose>
			<when test="queryType == 1">
				ORDER BY SEQ_NO DESC,CREATE_TIME DESC
			</when>
			<when test="queryType == 2">
				ORDER BY SEQ_NO DESC,IS_TOP,(CASE WHEN AGENCY_STATUS in(2)THEN 1 else 0 END ) DESC
			</when>
			<when test="queryType == 3">
				ORDER BY SEQ_NO DESC,PRICE DESC
			</when>
			<otherwise>
				ORDER BY SEQ_NO DESC ,IS_TOP,AGENCY_STATUS DESC,CREATE_TIME DESC
			</otherwise>
		</choose>
  </select>
  
  <select id="getDistBuildByComp" parameterType="java.util.Map" resultMap="BaseResultMap">
  	SELECT A.AGENCY_STATUS, A.BUILD_ID,A.CITY_ID,A.BUILD_NAME,A.BUILD_SOURCE,A.PHONE_LIMIT_MODE
	FROM BUILDING_INFO A
	LEFT JOIN BUILDING_BOUND B ON A.BUILD_ID = B.BUILD_ID
	LEFT JOIN BUILDING_COMP C ON A.BUILD_ID = C.BUILD_ID
	LEFT JOIN COOPERATION_RULE D ON A.CITY_ID = D.CITY_ID
	AND A.BUILD_ID = D.BUILD_ID
	AND D.COOPERATE_END IS NOT NULL
	AND D.COOPERATE_START IS NOT NULL
	LEFT JOIN FUN_COMP_REAL E ON A.CITY_ID = E.CITY_ID
	AND A.DEVLOPER_ID = E.DEVLOPER_ID
	WHERE ((B.CITY_ID = #{cityId} OR B.CITY_ID = 0) OR ( A.SYSTEM_flag = 1 AND A.ERP_COMP_ID = #{compId} OR C.COMP_NO = #{compNo} ))
	<![CDATA[and A.agency_status >= 1 AND A.BUILD_STATUS = 2 AND D.COOPERATE_END > convert(char(10),GETDATE(),120)]]>
	ORDER BY A.IS_TOP, A.AGENCY_STATUS DESC, A.CREATE_TIME DESC
  </select>
