    where ID = #{id,jdbcType=INTEGER}
  </update>
  
  <update id="unBindNumber">
  	update ALIYUN_PHONE set IDENTIFY_NAME = null,IDENTIFY_ID = null,NUMBER_STATE = 0,BIND_A = null,BIND_N_OR_B = null,
  	BIND_ARCHIVE_ID = null, BIND_USER_ID = null, BIND_USER_NAME = null, BIND_A_ORG_ID = null, BIND_A_ORG_NO = null
  	where BIND_X = #{bindX}
  </update>
  
  <update id="releaseNumber">
  	update ALIYUN_PHONE set IDENTIFY_NAME = null,IDENTIFY_ID = null,NUMBER_STATE = 0,BIND_A = null,BIND_N_OR_B = null,
  	BIND_ARCHIVE_ID = null, BIND_USER_ID = null, BIND_USER_NAME = null, BIND_A_ORG_ID = null, BIND_A_ORG_NO = null,ISRELEASE = 1, RELEASE_DATE = getdate()
  	where BIND_X = #{bindX}
  </update>
  
  <select id="getPhoneNumber" resultMap="BaseResultMap">
  	select <include refid="Base_Column_List" /> from ALIYUN_PHONE where COMP_ID = #{compId} and BIND_X = #{bindX} and ISRELEASE = 0
  </select>
  
  <select id="getPhoneNumberByBindA" resultMap="BaseResultMap">
  	select <include refid="Base_Column_List" /> from ALIYUN_PHONE where COMP_ID = #{compId} and BIND_A = #{bindA} and ISRELEASE = 0 and NUMBER_STATE = 1
  </select>
  
  <select id="getUnbindPhoneNumber" resultMap="BaseResultMap">
  	select top 1 <include refid="Base_Column_List" /> from ALIYUN_PHONE where COMP_ID = #{compId} and ORG_ID = #{orgId} and ISRELEASE = 0 and NUMBER_STATE = 0
  </select>
  
  <resultMap extends="BaseResultMap" id="DtoBaseResultMap" type="com.myfun.repository.erpdb.dto.AliyunPhoneDto">
  	<result column="USER_ID" jdbcType="INTEGER" property="userId" />
  	<result column="USER_NAME" jdbcType="VARCHAR" property="userName" />
  	<result column="ORGANIZATION_ID" jdbcType="INTEGER" property="organizationId" />
  	<result column="USER_POSITION" jdbcType="VARCHAR" property="userPosition" />
  	<result column="USER_MOBILE" jdbcType="VARCHAR" property="userMobile" />
  </resultMap>
  
  <select id="getAxnManageList" resultMap="DtoBaseResultMap">
  	  select a.USER_ID,a.USER_NAME,a.ORGANIZATION_ID,a.USER_POSITION,a.USER_MOBILE,b.NUMBER_STATE,b.BIND_X,b.BIND_A from FUN_USERS a left join ALIYUN_PHONE b 
  	  on a.COMP_ID = b.COMP_ID and a.USER_ID = b.BIND_USER_ID where a.COMP_ID = #{param.compId} and a.USER_WRITEOFF = 0
  	  <if test="param.bindFlag == 0">
  	  	and b.NUMBER_STATE is null
  	  </if>
  	  <if test="param.bindFlag == 1">
  	  	and b.NUMBER_STATE = 1
  	  </if>
  	  <if test="param.organizationId != null">
  	    and a.TISSUE_LINE like CONCAT('%:', #{param.organizationId}, ':%')
  	  </if>
  	  <if test="param.userId != null">
  	  	and a.USER_ID = #{param.userId}
  	  </if>
  	  <if test="param.userPosition != null and param.userPosition != ''">
  	  	and a.USER_POSITION = #{param.userPosition}
  	  </if>
  	  <if test="param.keys != null and param.keys != ''">
		and (a.USER_NAME like '%${param.keys}%' or b.BIND_X like '%${param.keys}%' or b.BIND_A like '%${param.keys}%' or a.USER_MOBILE like '%${param.keys}%')
  	  </if>
  	  order by a.USER_ID
  </select>
  
  <select id="getNumberManageList" resultMap="BaseResultMap">
  	  select a.BIND_USER_ID,a.BIND_ARCHIVE_ID,a.NUMBER_POOL_KEY,a.SUBS_ID,a.BIND_X,a.BIND_A from ALIYUN_PHONE a left join FUN_USERS b 
  	  on a.COMP_ID = b.COMP_ID and a.BIND_USER_ID = b.USER_ID where a.COMP_ID = #{compId} and a.ISRELEASE = 0
  	  <if test="numberPoolKey != null and numberPoolKey != ''">
  	  	and a.NUMBER_POOL_KEY = #{numberPoolKey}
  	  </if>
  	  <if test="keys != null and keys != ''">
		and (b.USER_NAME like '%${keys}%' or a.BIND_X like '%${keys}%' or a.BIND_A like '%${keys}%' or b.USER_MOBILE like '%${keys}%')
  	  </if>
  	  order by a.ID
  </select>
  <select id="getTop1Available" resultMap="BaseResultMap">
    select top 1 * from ALIYUN_PHONE where comp_id = #{compId} and NUMBER_STATE = 1
  </select>
  
</mapper>