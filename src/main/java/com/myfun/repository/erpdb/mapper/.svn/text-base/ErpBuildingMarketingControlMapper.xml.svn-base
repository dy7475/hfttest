<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myfun.repository.erpdb.dao.ErpBuildingMarketingControlMapper">
  <resultMap id="BaseResultMap" type="com.myfun.repository.erpdb.po.ErpBuildingMarketingControl">
    <!--@mbggenerated-->
    <id column="ID" jdbcType="INTEGER" property="id" />
    <result column="CITY_ID" jdbcType="INTEGER" property="cityId" />
    <result column="COMP_ID" jdbcType="INTEGER" property="compId" />
    <result column="BUILD_ID" jdbcType="INTEGER" property="buildId" />
    <result column="CASE_TYPE" jdbcType="TINYINT" property="caseType" />
    <result column="CASE_ID" jdbcType="INTEGER" property="caseId" />
    <result column="CASE_NO" jdbcType="VARCHAR" property="caseNo" />
    <result column="CASE_STATUS" jdbcType="TINYINT" property="caseStatus" />
    <result column="CREATION_TIME" jdbcType="TIMESTAMP" property="creationTime" />
    <result column="UPDATE_TIME" jdbcType="TIMESTAMP" property="updateTime" />
    <result column="MARKETING_CONTROL_MD5" jdbcType="VARCHAR" property="marketingControlMd5" />
    <result column="BUILDING_SET_ROOF" jdbcType="VARCHAR" property="buildingSetRoof" />
    <result column="BUILDING_SET_UNIT" jdbcType="VARCHAR" property="buildingSetUnit" />
    <result column="BUILDING_SET_FLOOR" jdbcType="VARCHAR" property="buildingSetFloor" />
    <result column="ROOM_ID" jdbcType="VARCHAR" property="roomId" />
  </resultMap>
  <sql id="Example_Where_Clause">
    <!--@mbggenerated-->
    <where>
      <foreach collection="oredCriteria" item="criteria" separator="or">
        <if test="criteria.valid">
          <trim prefix="(" prefixOverrides="and" suffix=")">
            <foreach collection="criteria.criteria" item="criterion">
              <choose>
                <when test="criterion.noValue">
                  and ${criterion.condition}
                </when>
                <when test="criterion.singleValue">
                  and ${criterion.condition} #{criterion.value}
                </when>
                <when test="criterion.betweenValue">
                  and ${criterion.condition} #{criterion.value} and #{criterion.secondValue}
                </when>
                <when test="criterion.listValue">
                  and ${criterion.condition}
                  <foreach close=")" collection="criterion.value" item="listItem" open="(" separator=",">
                    #{listItem}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>
  <sql id="Update_By_Example_Where_Clause">
    <!--@mbggenerated-->
    <where>
      <foreach collection="example.oredCriteria" item="criteria" separator="or">
        <if test="criteria.valid">
          <trim prefix="(" prefixOverrides="and" suffix=")">
            <foreach collection="criteria.criteria" item="criterion">
              <choose>
                <when test="criterion.noValue">
                  and ${criterion.condition}
                </when>
                <when test="criterion.singleValue">
                  and ${criterion.condition} #{criterion.value}
                </when>
                <when test="criterion.betweenValue">
                  and ${criterion.condition} #{criterion.value} and #{criterion.secondValue}
                </when>
                <when test="criterion.listValue">
                  and ${criterion.condition}
                  <foreach close=")" collection="criterion.value" item="listItem" open="(" separator=",">
                    #{listItem}
                  </foreach>
                </when>
              </choose>
            </foreach>
          </trim>
        </if>
      </foreach>
    </where>
  </sql>
  <sql id="Base_Column_List">
    <!--@mbggenerated-->
    ID, CITY_ID, COMP_ID, BUILD_ID, CASE_TYPE, CASE_ID, CASE_NO, CASE_STATUS, CREATION_TIME,
    UPDATE_TIME, MARKETING_CONTROL_MD5, BUILDING_SET_ROOF, BUILDING_SET_UNIT, BUILDING_SET_FLOOR,
    ROOM_ID
  </sql>

  <resultMap id="ControlInfoMap" extends="BaseResultMap" type="com.myfun.erpWeb.businesstools.builddic.vo.ErpBuildingMarketingControlDto"/>

  <select id="getRoomConInfo" resultMap="ControlInfoMap">
    select
    ID buildingControlId, BUILD_ID, CASE_TYPE, CASE_ID, CASE_NO, CASE_STATUS,
    MARKETING_CONTROL_MD5, BUILDING_SET_ROOF, BUILDING_SET_UNIT, BUILDING_SET_FLOOR, ROOM_ID
    from BUILDING_MARKETING_CONTROL
    where COMP_ID = #{compId} and BUILD_ID = #{buildId}
    <if test="buildingSetRoof != null">
      and BUILDING_SET_ROOF = #{buildingSetRoof}
    </if>
    <if test="buildingSetUnit != null">
      and BUILDING_SET_UNIT = #{buildingSetUnit}
    </if>
    <!-- 删除状态的不显示 -->
    AND CASE_STATUS != 7
  </select>

  <resultMap id="RoofInfoMap" extends="BaseResultMap" type="com.myfun.erpWeb.businesstools.builddic.vo.GetRoofInfoVo">
    <result column="HOUSE_YEAR" jdbcType="INTEGER" property="houseYear" />
    <result column="HOUSE_USEAGE" jdbcType="INTEGER" property="houseUseage" />
    <result column="UNITS" jdbcType="INTEGER" property="units" />
    <result column="DOORS" jdbcType="INTEGER" property="doors" />
  </resultMap>
  <select id="getRoofConInfo" resultMap="RoofInfoMap">
    select
    	SUM (iif(CASE_STATUS not in (5,6,7) and CASE_TYPE = 1,1,0)) AS saleCount,
    	SUM (iif(CASE_STATUS not in (5,6,7) and CASE_TYPE = 2,1,0)) AS leaseCount,
    	SUM (iif(CASE_STATUS = 5,1,0)) AS innerDealCount,
    	SUM (iif(CASE_STATUS = 6,1,0)) AS outerDealCount
    FROM BUILDING_MARKETING_CONTROL
    where COMP_ID = #{compId}
    and BUILD_ID = #{buildId}
    and BUILDING_SET_ROOF = #{buildingSetRoof}
  </select>

  <select id="getUnitConInfo" resultMap="RoofInfoMap">
    select
        SUM (iif(CASE_STATUS not in (5,6,7) and CASE_TYPE = 1,1,0)) AS saleCount,
    	SUM (iif(CASE_STATUS not in (5,6,7) and CASE_TYPE = 2,1,0)) AS leaseCount,
    	SUM (iif(CASE_STATUS = 5,1,0)) AS innerDealCount,
    	SUM (iif(CASE_STATUS = 6,1,0)) AS outerDealCount
    FROM BUILDING_MARKETING_CONTROL
    where COMP_ID = #{compId}
    and BUILD_ID = #{buildId}
    and BUILDING_SET_UNIT = #{buildingSetUnit}
  </select>

  <update id="batchUpdateRoomMd5" parameterType="list">
    <foreach close="" collection="batchUpdateRoomMd5s" index="index" item="item" open="" separator=";">
      update BUILDING_MARKETING_CONTROL
      <set>
        MARKETING_CONTROL_MD5 = #{item.addMd5},
        <if test="item.buildRoof != null">
         BUILDING_SET_ROOF = #{item.buildRoof},
        </if>
        <if test="item.buildUnit != null">
           BUILDING_SET_UNIT = #{item.buildUnit},
        </if>
        <if test="item.buildFloor != null">
           BUILDING_SET_FLOOR = #{item.buildFloor},
        </if>
        <if test="item.roomId != null">
         ROOM_ID = #{item.roomId},
      </if>
        update_time = GETDATE()
      </set>
      WHERE COMP_ID = #{compId}
      AND BUILD_ID = #{buildId}
      AND MARKETING_CONTROL_MD5 = #{item.oldAddMd5}
    </foreach>
  </update>
  <delete id="deleteById">
    DELETE FROM BUILDING_MARKETING_CONTROL
    WHERE COMP_ID = #{compId} AND BUILD_ID = #{buildId}
  </delete>
  <delete id="delete">
    DELETE FROM BUILDING_MARKETING_CONTROL
    WHERE COMP_ID = #{compId} AND BUILD_ID = #{buildId}
    <if test="marketingControl.buildingSetRoof != null">
      AND BUILDING_SET_ROOF = #{marketingControl.buildingSetRoof}
    </if>
    <if test="marketingControl.buildingSetUnit != null">
      AND BUILDING_SET_UNIT = #{marketingControl.buildingSetUnit}
    </if>
    <if test="marketingControl.buildingSetFloor != null">
      AND BUILDING_SET_FLOOR = #{marketingControl.buildingSetFloor}
    </if>
    <if test="marketingControl.roomId != null">
      AND ROOM_ID = #{marketingControl.roomId}
    </if>
  </delete>
</mapper>