  <select id="getUserSalesOnWebsite" resultType="com.myfun.repository.erpdb.dto.HouseCaseIdDto">
      select DISTINCT A.SALE_ID houseId from FUN_SALE A
      JOIN FUN_SALE_PUBLISH_INFO F ON A.SALE_ID=F.SALE_ID
      WHERE F.WEBSITE_STATE='1' AND A.USER_ID=#{userId}
  </select>

  <select id="getWaitDownHouseList" resultType="com.myfun.repository.erpdb.dto.HouseCaseIdDto">
      select DISTINCT A.SALE_ID houseId from FUN_SALE A
      JOIN FUN_SALE_PUBLISH_INFO M ON A.SALE_ID=M.SALE_ID AND M.WEBSITE_STATE='1'
      WHERE  A.SALE_ID IN
        <foreach close=")" collection="salesId" item="listItem" open="(" separator=",">
          #{listItem}
        </foreach>
      AND NOT EXISTS (
      SELECT 1 FROM SALE_PEOPLE_RELATIVE B
      JOIN FUN_USERS D ON B.USER_ID = D.USER_ID AND D.USER_WRITEOFF = 0
      WHERE B.SALE_ID = A.SALE_ID AND B.PEOPLE_TYPE IN(2,3) AND B.DEL_FLAG=0
      ) AND NOT EXISTS (
      SELECT 1 FROM FUN_ENTRUST C
      JOIN FUN_USERS E ON C.RECORD_USER = E.USER_ID AND E.USER_WRITEOFF = 0
      WHERE A.SALE_ID=C.CASE_ID  AND C.CASE_TYPE=1 AND C.IS_EFFECTIVE=1
      )
  </select>

  <select id="getNotWriteOffHouseList" resultType="com.myfun.repository.erpdb.dto.HouseCaseIdDto">
      select DISTINCT A.SALE_ID houseId from FUN_SALE A
      LEFT JOIN SALE_PEOPLE_RELATIVE B ON A.SALE_ID=B.SALE_ID AND B.DEL_FLAG=0
      LEFT JOIN FUN_ENTRUST C ON A.SALE_ID=C.CASE_ID AND  CASE_TYPE=1 AND IS_EFFECTIVE=1
      LEFT JOIN FUN_USERS D ON B.USER_ID=D.USER_ID
      LEFT JOIN FUN_USERS E ON C.RECORD_USER=E.USER_ID
      LEFT JOIN FUN_SALE_PUBLISH_INFO F ON A.SALE_ID=F.SALE_ID
      WHERE B.PEOPLE_TYPE IN(2,3) AND D.USER_WRITEOFF=0 AND E.USER_WRITEOFF=0 AND F.WEBSITE_STATE='1'
        AND A.USER_ID=#{userId}
  </select>
