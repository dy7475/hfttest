		    A.MANAGE_LOCATION_ATT_RESULT as manageLocationAttResult,A.MANAGE_TIME_ATT_RESULT as manageTimeAttResult
		    ,A.EXTRA_PATH as extraPath,A.EXTRA_TYPE as extraType,A.ATT_REMARK as attRemark,A.EDIT_REMARK as editRemark,A.BELATE_MINUTE as belateMinute,A.ATT_NUM as attNum,A.ATT_CLASS_NAME as attClassName
		    ,A.LOCATION_DESC as locationDesc,A.ATT_CONFIG_LOCATION as attConfigLocation
		    FROM FUN_ATTENDANCE_RECORD_DETAIL A JOIN t_max_id B ON A.ATT_ID = B.ID
		    WHERE A.COMP_ID =#{compId} order by A.ATT_ID asc
	  </if>
  </select>
  

  
	<select id="getRecordByUserId" resultMap="BaseResultMap">
		SELECT
		<include refid="Base_Column_List" />
		FROM FUN_ATTENDANCE_RECORD_DETAIL where USER_ID=#{userId} and ATT_OUGHT_DATE=#{attDate}
	</select>

	<select id="getAttendanceALLRecordByMonth" resultMap="BaseResultMap">
		SELECT
			<include refid="Base_Column_List" />
		FROM FUN_ATTENDANCE_RECORD_DETAIL
		where
		USER_ID=#{userId} and
		ATT_OUGHT_DATE BETWEEN #{startDate}
		and
		#{endDate}
	</select>
	
	<select id="getAttendanceRecordByUserId" resultMap="BaseResultMap">
		SELECT
		<include refid="Base_Column_List" />
		FROM FUN_ATTENDANCE_RECORD_DETAIL
		where
		USER_ID = #{userId} and ATT_OUGHT_DATE=#{attDate}
	</select>
	
	<select id="getAttendanceRecordStatisticalAnalysisDetailOffOneDay" resultType="map">
		select
			MAX(ATT_RESULT_WEIGHT) as attResultWeight, USER_ID as userId
		from FUN_ATTENDANCE_RECORD_DETAIL 
			where
				COMP_ID = #{compId} 
		   	<if test="areaId!=null">
		   		and AREA_ID = #{areaId}
		   	</if>
		   	<if test="regId!=null">
		   		and REG_ID = #{regId}
		   	</if>
		   	<if test="deptId!=null">
		   		and DEPT_ID = #{deptId}
		   	</if>
		   	<if test="grId!=null">
		   		and GR_ID = #{grId}
		   	</if>
		   	<if test="userId!=null">
		   		and USER_ID = #{userId}
		   	</if>
				and ATT_OUGHT_DATE = #{attDate} group by USER_ID
	</select>
	
	<select id="getAttendanceRecordStatisticalAnalysisDetailListOffOneDay" resultType="map">
		
		select
			MAX(ATT_RESULT_WEIGHT) as attResultWeight, USER_ID as userId
		from FUN_ATTENDANCE_RECORD_DETAIL 
			where
				COMP_ID = #{compId} 
		   	<if test="areaId!=null">
		   		and AREA_ID = #{areaId}
		   	</if>
		   	<if test="regId!=null">
		   		and REG_ID = #{regId}
		   	</if>
		   	<if test="deptId!=null">
		   		and DEPT_ID = #{deptId}
		   	</if>
		   	<if test="grId!=null">
		   		and GR_ID = #{grId}
		   	</if>
		   	<if test="userId!=null">
		   		and USER_ID = #{userId}
		   	</if>
				and ATT_OUGHT_DATE = #{attDate} group by USER_ID 
			<if test="attWeightTypes!=null">
			having max(ATT_RESULT_WEIGHT) IN
				<foreach close=")" collection="attWeightTypes" item="attWeightType" open="(" separator=",">
		   			 #{attWeightType}
		   		</foreach>
		   	</if>
				
	</select>
	
	<select id="getAttendanceRecordStatisticalAnalysisDetailOffOneMonth" resultType="map">
		select
			max(ATT_RESULT_WEIGHT) as attType, CONVERT(VARCHAR(10),ATT_OUGHT_DATE,120) as attDate
		from FUN_ATTENDANCE_RECORD_DETAIL 
			where
				COMP_ID = #{compId} and USER_ID = #{userId} and ATT_OUGHT_DATE &gt;= #{startDate} and ATT_OUGHT_DATE &lt; #{endDate} group by ATT_OUGHT_DATE
	</select>
	
	<select id="getAttendanceRecordDetailListMapByRange" resultType="map">
	 	 with t_max_id as (
		  	select max(ATT_ID) as ID,ATT_NUM,ATT_ATTRIBUTE,USER_ID from FUN_ATTENDANCE_RECORD_DETAIL where
		 	COMP_ID =#{compId} and ATT_OUGHT_DATE = #{attDate} 
		 	<if test="userIds!=null">
		   	 	AND USER_ID in
			    <foreach close=")" collection="userIds" item="userId" open="(" separator=",">
			    	#{userId}
			    </foreach>
		    </if>
		   	group by ATT_NUM,ATT_ATTRIBUTE,USER_ID
		   )
	    SELECT A.ATT_ID as attId,A.AREA_ID as areaId,A.REG_ID as regId,A.DEPT_ID as deptId,A.GR_ID as grId,A.USER_ID as userId,A.ARCHIVE_ID as archiveId,A.ATT_OUGHT_DATE as attOughtDate,A.SERVER_ATT_TIME as serverAttTime,A.CONFIG_ATT_TIME as configAttTime,A.ATT_ATTRIBUTE as attAttribute,
		    A.MANAGE_LOCATION_ATT_RESULT as manageLocationAttResult,A.MANAGE_TIME_ATT_RESULT as manageTimeAttResult
		    ,A.EXTRA_PATH as extraPath,A.EXTRA_TYPE as extraType,A.ATT_REMARK as attRemark,A.BELATE_MINUTE as belateMinute,A.ATT_NUM as attNum
		    ,A.LOCATION_DESC as locationDesc,A.ATT_CONFIG_LOCATION as attConfigLocation
		    FROM FUN_ATTENDANCE_RECORD_DETAIL A JOIN t_max_id B ON A.ATT_ID = B.ID
	    WHERE A.COMP_ID =#{compId} and A.ATT_OUGHT_DATE = #{attDate} 
  </select>
  <select id="getAttendanceRecordStatisticalCountGroupByWeight" resultType="map">
		select
			count(1) as attResultCount, ATT_RESULT_WEIGHT as attResultWeight
		from FUN_ATTENDANCE_RECORD_DETAIL 
			where
				COMP_ID = #{compId} 
		   	<if test="userId!=null">
		   		and USER_ID = #{userId}
		   	</if>
		   		AND MANAGE_TIME_ATT_RESULT!='0'
		   		AND ATT_RESULT_WEIGHT!=4 AND ATT_RESULT_WEIGHT!=1
				and ATT_OUGHT_DATE &gt;= #{startDate} and ATT_OUGHT_DATE &lt; #{endDate} group by ATT_RESULT_WEIGHT
  </select>
</mapper>