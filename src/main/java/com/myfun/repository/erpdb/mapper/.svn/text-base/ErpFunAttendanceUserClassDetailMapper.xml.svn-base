        <include refid="Base_Column_List"/>
        from FUN_ATTENDANCE_USER_CLASS_DETAIL
        <where>
            <if test="1==1">
                and comp_id=#{compId} and IS_DEL =0
            </if>
            <if test="null != groupId">
                and ATT_GR_ID=#{groupId}
            </if>
            <if test="null != month and '' != month">
