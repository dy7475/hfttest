	<select id="countHshApplied" resultType="java.lang.Integer">
		select count(1)
		from FUN_ATTENDANCE_USER_CLASS_DETAIL A join FUN_ATTENDANCE_GROUP B
		ON A.ATT_GR_ID = B.ID
		WHERE  A.COMP_ID = #{compId} and A.ATT_ClASS_ID = #{attendanceClassId} and A.IS_DEL = 0 AND B.IS_DEL = 0
	</select>

	<select id="getUserClassListByUsers" resultMap="BaseResultMap">
