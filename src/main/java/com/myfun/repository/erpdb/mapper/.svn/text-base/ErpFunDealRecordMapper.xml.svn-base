	<select id="getMgrDealNoteList" resultMap="erpFunDealRecordDtoMap">
		select 
		UPDATE_CLASSIC,UPDATE_USER_NAME,TRACK_CONTENT,UPDATE_TYPE,UPDATE_DATE,AGREEMENT_NO,RECORD_ID,FRONT_RECORD_ID,PF_ACTUAL,UPDATE_UID,DEPT_ID
		from FUN_DEAL_RECORD
		where comp_id=#{compId} and AGREEMENT_NO=#{dealId}
		<if test="logType == 1">AND UPDATE_TYPE in (0,3)</if><!-- 获取合同记录日志 -->
		<if test="logType == 2">AND UPDATE_TYPE in (1)</if><!-- 应收实收日志 -->
		<if test="logType == 3">AND UPDATE_TYPE in (2)</if><!-- 业绩日志 -->
		<if test="logType == 4">AND UPDATE_TYPE in (4,5)</if><!-- 税费日志 -->
		ORDER BY UPDATE_DATE DESC
	</select>

	<select id="getLastRecordId" resultMap="BaseResultMap">
		select top 1 RECORD_ID from FUN_DEAL_RECORD where AGREEMENT_NO = '${agreementNo}' and UPDATE_TYPE=#{updateType}
		<if test="pfActual != null">
			and PF_ACTUAL = #{pfActual}
		</if>
		order by UPDATE_DATE desc
	</select>

