  <select id="getAttendanceScopeList" resultMap="extBaseResultMap">
    select DISTINCT a.ATT_SCOPE_ID, a.ATT_SCOPE_X, a.ATT_SCOPE_Y, a.ATT_DISTANCE, a.LOCATION_DESC, a.LOCATION_DESC_ABB,
    a.CREATE_TIME, a.COMP_ID, a.CAL_DEL, a.IS_DEL, a.DEPT_ID, a.DEL_TIME from FUN_ATTENDANCE_SCOPE a
    left join FUN_ATTENDANCE_SCOPE_BIZ b on a.ATT_SCOPE_ID = b.ATT_SCOPE_ID and b.is_del=0
    left join FUN_ATTENDANCE_SCOPE_BIZ_PERSON c on b.id=c.SCOP_BIZ_ID and c.is_del=0
    <where>
        a.IS_DEL = 0 and a.COMP_ID=#{compId}
      <if test="null != keyWords and '' != keyWords">
         and (a.LOCATION_DESC_ABB like '%${keyWords}%' or c.USER_NAME like '%${keyWords}%')
      </if>
    </where>
    order by ATT_SCOPE_ID
  </select>
