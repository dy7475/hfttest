      PERFORMANCE_AMOUNT = #{performanceAmount,jdbcType=NUMERIC},
      INCOME_AMOUNT_TYPE = #{incomeAmountType,jdbcType=TINYINT},
      USER_ID = #{userId,jdbcType=INTEGER},
      PERFORMANCE_ID = #{performanceId,jdbcType=INTEGER},
      PERFORMANCE_NAME = #{performanceName,jdbcType=VARCHAR},
      INCOME_TIME = #{incomeTime,jdbcType=TIMESTAMP},
      ISSUED_AMOUNT = #{issuedAmount,jdbcType=NUMERIC},
      CREATE_TIME = #{createTime,jdbcType=TIMESTAMP},
      PF_ID = #{pfId,jdbcType=INTEGER},
      NEED_INCOME_AMOUNT = #{needIncomeAmount,jdbcType=NUMERIC},
      DEL_FLAG = #{delFlag,jdbcType=TINYINT},
      DEL_DATA = #{delData,jdbcType=TIMESTAMP},
      ASSESSMENT_MONTH = #{assessmentMonth,jdbcType=VARCHAR},
      ARCHIVE_ID = #{archiveId,jdbcType=INTEGER},
      PROFIT_ID = #{profitId,jdbcType=INTEGER}
    where ID = #{id,jdbcType=INTEGER}
  </update>

  <select id="selectSumUserIncome" resultType="java.math.BigDecimal">
    SELECT SUM(ISNULL(INCOME_AMOUNT,0)) AS sumIncome FROM FUN_PROFIT_DYNAMIC_DETAIL
	 WHERE COMP_ID = #{compId} AND DEL_FLAG = 0 AND INCOME_AMOUNT_TYPE = 1 AND SETTLEMENT_MONTH = #{queryMonth} AND ARCHIVE_ID = #{archiveId}
  </select>

  <select id="selectCompPerformanceRanking" resultType="com.myfun.erpWeb.managecenter.profit.vo.CompPerformanceRankingVO">
    WITH  A AS (
	   	SELECT USER_ID ,SUM(ISNULL(INCOME_AMOUNT,0)) userIncomeAmount FROM FUN_PROFIT_DETAIL
		WHERE COMP_ID = #{compId} AND DEL_FLAG = 0  AND SETTLEMENT_MONTH = #{queryMonth}
		GROUP BY USER_ID
	) 
	SELECT ROW_NUMBER() OVER (ORDER BY ISNULL(A.userIncomeAmount,0)  DESC) AS rank, A.USER_ID userId FROM A  
	ORDER by ISNULL(A.userIncomeAmount,0)   DESC

  </select>

</mapper>