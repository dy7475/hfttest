    LEFT JOIN POS_PAYMENT_ORDER b on a.EARBEST_ID = b.FINANCE_ID and FINANCE_TYPE = 1 and pay_status!=3
    <if test="'CREATE_USER'==param.userType">
      JOIN FUN_USERS C ON C.USER_ID=A.CREATE_USER
