				<when test="operIds.contains('VIEW_COMP_DEAL')">
					AND (COMP_ID=#{param.compId} OR USER_ID = #{param.userId})
				</when>
				<when test="operIds.contains('VIEW_AREA_DEAL')">
					AND (AREA_ID=#{param.areaId} OR USER_ID = #{param.userId})
				</when>
				<when test="operIds.contains('VIEW_REG_DEAL')">
					AND (REG_ID=#{param.regId} OR USER_ID = #{param.userId})
				</when>
				<when test="operIds.contains('VIEW_DEPT_DEAL')">
					AND (DEPT_ID=#{param.deptId} OR USER_ID = #{param.userId})
				</when>
				<when test="operIds.contains('VIEW_GROUP_DEAL')">
					AND ((DEPT_ID=#{param.deptId} AND GR_ID = #{param.grId}) OR USER_ID = #{param.userId})
				</when>
				<otherwise>
					AND USER_ID = #{param.userId}
				</otherwise>
			</choose>
		</where>
	</select>
</mapper>