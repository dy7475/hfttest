  <select id="getValidErpFunVideoByCondition" parameterType="com.myfun.repository.erpdb.po.ErpFunVideo" resultMap="BaseResultMap">
  	select <include refid="Base_Column_List" /> from FUN_VIDEO
	    <where>
	    	<if test="null != videoId">
				and VIDEO_ID = #{videoId,jdbcType=INTEGER}
			</if>
			<if test="null != caseId">
				and CASE_ID = #{caseId,jdbcType=INTEGER}
			</if>
			<if test="null != caseType">
				and CASE_TYPE = #{caseType,jdbcType=TINYINT}
			</if>
				and VIDEO_STATUS = 0
		</where>
  </select>
  	<select id="getVideoListOrderByVideoIdDesc" parameterType="java.lang.Integer" resultMap="BaseResultMap">
		select top 50 a.VIDEO_ID,a.VIDEO_ADDR,a.PHOTO_ADDR
		from FUN_VIDEO a left join FUN_VIDEO_FASTDFS b on b.video_id=a.video_id 
		where b.video_id is null
		<if test="dealId != null and dealId &gt; 0">AND a.VIDEO_ID &lt; #{videoId}</if>
		and FILE_SYSTEM_TYPE = 0
		order by VIDEO_ID desc
	</select>
 	<resultMap extends="BaseResultMap" id="BaseResultVO" type="com.myfun.erpWeb.house.vo.ErpFunVideoVo">
 	</resultMap>
	<select id="getVideoList" resultMap="BaseResultVO">
		select
		<include refid="Base_Column_List" />
		from FUN_VIDEO
		where CASE_ID = #{param.caseId} and CASE_TYPE = #{param.caseType} 
		<choose>
			<when test="param.isMeiLian != null and param.isMeiLian == 1">
				and VIDEO_STATUS in(0,5) 	
			</when>
			<otherwise>
				and VIDEO_STATUS  = 0 
			</otherwise>
		</choose>
		order by VIDEO_ID ASC
	</select>
	
	<select id="getVideoCount" resultType="java.lang.Integer">
		select
		count(1)
		from FUN_VIDEO
		where CASE_ID = #{param.caseId} and CASE_TYPE = #{param.caseType} and
		VIDEO_STATUS = 0 and VIDEO_REVIEW_FLAG = 1
	</select>
	<update id="delVideo">
		UPDATE B SET B.IS_DEL = 1 
		FROM FUN_VIDEO A JOIN FUN_VIDEO_ML B ON A.VIDEO_ID = B.VIDEO_ID
		WHERE  B.IS_DEL = 0 AND A.VIDEO_REVIEW_FLAG = 0 AND B.PHOTOGRAPHER_FLAG = 0 AND A.VIDEO_STATUS = -1
		AND A.VIDEO_ID  =  #{videoId} 
	</update>
