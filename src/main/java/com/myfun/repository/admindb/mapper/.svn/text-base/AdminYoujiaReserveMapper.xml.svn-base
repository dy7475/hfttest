  <select id="getDaiKanList" resultMap="extBaseResultMap">
    SELECT a.id,a.YOUJIA_USER_ID,a.RESERVE_NAME,a.RESERVE_PHONE, c.headimgurl WX_PHOTO, a.RESERVE_TIME
    FROM YOUJIA_RESERVE a
    JOIN YOUJIA_RESERVE_HOUSE_EVA b on a.ID = b.RESERVER_ID
    LEFT JOIN  w_microshop_member c on a.YOUJIA_USER_ID=c.uid
    <where>
      <if test="null != userId">
        AND b.HOUSE_USER_ID=#{userId}
      </if>
      <if test="null != compId">
        AND a.COMP_ID=#{compId}
      </if>
      <if test="null != grId">
        AND b.GR_ID=#{grId}
      </if>
      <if test="null != keyWords and '' != keyWords">
        AND (RESERVE_NAME like '%${keyWords}%' or RESERVE_PHONE like '%${keyWords}%')
      </if>
      <if test="null != startTime">
        AND a.RESERVE_TIME &gt;= #{startTime}
      </if>
      <if test="null != endTime">
        AND a.RESERVE_TIME &lt;= #{endTime}
      </if>
    </where>
    GROUP BY a.id,a.YOUJIA_USER_ID,a.RESERVE_NAME,a.RESERVE_PHONE,c.headimgurl, a.RESERVE_TIME
    order by a.id desc
  </select>

  <select id="selectNotFinish" resultMap="BaseResultMap">
