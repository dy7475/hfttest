  <select id="sumPaid" resultMap="paidCountMap">
  	SELECT PAID_CATIGORY, PAID_TYPE, isnull(SUM(AMOUNT), 0) AS SUM_AMOUNT FROM FUN_PAID
  	where 1 = 1
  	<choose> 
