package com.myfun.erpWeb.managecenter.attendance.vo;


public class AttendanceGroupDto {

    /**
     * @mbggenerated 主键自增
     */
    private Integer id;
    /**
     * @mbggenerated 1行政班  2：排班
     */
    private Byte classType;
    /**
     * @mbggenerated 考勤组名
     */
    private String groupName;
    /**
     * 排班id(逗号隔开)
     */
    private String attClassId;

    public Integer getId() {
        return id;
    }

    public void setId(Integer id) {
        this.id = id;
    }

    public Byte getClassType() {
        return classType;
    }

    public void setClassType(Byte classType) {
        this.classType = classType;
    }

    public String getGroupName() {
        return groupName;
    }

    public void setGroupName(String groupName) {
        this.groupName = groupName;
    }

    public String getAttClassId() {
        return attClassId;
    }

    public void setAttClassId(String attClassId) {
        this.attClassId = attClassId;
    }
}
