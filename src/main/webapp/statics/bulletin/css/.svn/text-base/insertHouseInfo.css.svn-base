html {
	overflow: hidden;background-color: #EFF4F9;
}

body {
	font-size: 12px;
	margin: 0 5px;
	background-color: #EFF4F9;
	overflow-x: hidden;
}

.lt {
	float: left;
}

a {
	text-align: center;
}

div.title {
	font-size: 16px;
	font-weight: bold;
	color: #FB7155;
	margin-bottom: 5px;
}

td.left {
	font-size: 12px;
	width: 70px;
	padding: 2px;
}

td.right {
	font-size: 12px;
	padding: 2px;
}
.aHref{
	text-decoration: none;
	color: #444444;padding: 5px;
}
.aHref:HOVER{
	text-decoration: none;
	color: white;
	background-color: #259CF3;
}
.menu_zi_select{
	border: 1px solid #259CF3;border-radius: 3px;padding: 3px;text-decoration: none;color: #259CF3;
	padding-left: 6px;padding-right: 6px;	
}
.menu_zi_select:HOVER{
	border: 1px solid #259CF3;border-radius: 3px;padding: 3px;text-decoration: none;color: #259CF3;
	background-color: white;
	padding-left: 6px;padding-right: 6px;	
}
.divContent{
	width: 689px;height: 241px;margin-top: 5px;overflow-x: hidden;overflow-y: auto;
}
.divContent::-webkit-scrollbar {
	width: 8px;border-radius: 6px;
}
.divContent::-webkit-scrollbar-track {
	background-color: #DADADA;
}
.divContent::-webkit-scrollbar-thumb {
	background-color: #757474;
  	border-radius: 6px;
}
.divContent::-webkit-scrollbar-thumb:hover {
	background-color: #898989;
}
.tableCss {
	font-size: 12px;
	margin: 0 auto;
	width: 670px;
  	margin-left: 10px;
  	margin-top: 6px;
}

.tableCss th {
	color: #444444;
	height: 25px;
	font-size: 14px;
	padding-left: 3px;
}

.tableCss td {
	padding-left: 5px;
	text-align: left;
}

.tableCss .trStyle {
	background-color: white;
	height: 22px; line-height:32px;
	padding-left: 3px;cursor: pointer;
}

.tableCss .trStyle:HOVER {
	background: #F2F8FF;
	height: 22px;line-height:32px;
	padding-left: 3px;
	background-color: #D9F1FE;
}

.selectedTR td {
	background: #EEF3F7
}

.searchBtn {
	border: 1px solid #A9C9E2;
	background-color: #D4D0C8;
	height: 20px;
}

.scrollBar {
	overflow: hidden;
	background-image:url("../images/NewVersion/HouseBG.png");background-repeat:no-repeat;
	background-color: #ffffff;
}
/**弹出层**/
.Input_box1 {
	height: 15px;
	width: 110px;
	font-size: 12px;
	border: 1px solid #4A9AFD;
	background-color: #EAF0FE;
}

.Input_box2 {
	height: 15px;
	width: 50px;
	font-size: 12px;
	border: 1px solid #4A9AFD;
	background-color: #EAF0FE;
}

.Input_box3 {
	height: 15px;
	width: 35px;
	font-size: 12px;
	border: 1px solid #4A9AFD;outline:none;
	background-color: #FFFFFF;border-radius: 3px;
	box-shadow: 0 2px 3px rgb(221, 221, 221) inset;
}

.Input_box4 {
	height: 15px;
	width: 60px;
	font-size: 12px;
	border: 1px solid #4A9AFD;
	background-color: #EAF0FE;
}

.fd {
	margin-top: 2px;
	margin-left: 2px;
	float: left;
	overflow: visible;
	position: absolute;
	top: -1000;
	left: -1000;
	cursor: move;
	float: left;
	padding: 1px;
	display: none;
}

.fd1,.fd2,.fd3 {
	border-color: #888
}

.fd_filter {
	filter: alpha(opacity = 100);
	opacity: 1;
}

.fd ul {
	padding: 0px;
	margin: 0;
}

.fd ul li {
	list-style-type: none;
	padding: 5px;
	margin: 0;
	float: left;
	CURSOR: pointer;
	font-size: 12px;
	color: #000;
}

.close {
	padding: 4px;
	float: right;
	font-size: 14px;
	color: #f00;
	CURSOR: pointer;
}

#windows {
	border: 1px solid #4A9AFD;
	border-top: none;
	padding: 9px;
	font-size: 12px;
	color: #153A67;
	background: #eaf0fe;
}

#windows a {
	font-size: 12px;
	color: #153A67;
	line-height: 20px;
	text-decoration: none
}

#windows a:hover {
	color: #F00;
}

#windows .input {
	background: #FFF;
	border: 1px solid #153A67;
	height: 12px;
	font-size: 12px;
}

#windows .menu_zi_select {
	background: url(../images/bulletin_bg.gif) 0 -40px no-repeat;
	color: #fff;
	display: block;
	width: 44px;
	height: 20px;
	line-height: 20px;
	vertical-align: middle;
	text-decoration: none;
	float: right;
}

#windows .menu_zi_select:HOVER {
	background-position: 0 -60px;
	color: #fff;
}

.street_box {
	height: 15px;
	width: 130px;
	font-size: 12px;
	border: 1px solid #4A9AFD;
	background-color: #FFFFFF;
	color: #AAAAAA;
}

.bg {
	background: url(../../publicStyle/btn_bg.gif) no-repeat;
}

.btn_40red {
	text-align: center;
	background-position: 0 0;
	text-decoration: none;
	color: #153a67;
	width: 40px;
	height: 21px;
	line-height: 21px;
	display: block;
	text-align: center;
	float: right;
	margin-right: 5px
}
/*分页链接**/
.bbs_page {
	text-align: left;
	padding: 0;
	margin: 0 10px 0 5px;
	vertical-align: middle;
}

.bbs_page a,.bbs_page strong {
	padding: 2px 5px;
	margin-right: 2px;
	border: 1px solid;
	line-height: 20px;
	overflow: hidden;
	
}

.bbs_page a {
	border-color: #E6E7E1;
	background-color: #FFF;
	color: #09C;
	text-decoration: none;
}

.bbs_page a:hover {
	text-decoration: none;
}

.bbs_page strong {
	border-color: #09C;
	background-color: #09C;
	color: #FFF;
	font-weight: 700;
}

.bbs_page a.prev,.bbs_page a.next {
	background-repeat: no-repeat;
}

.bbs_page a.prev {
	background-position: 30% 50%;
	padding: 0;
	width: 22px;
	overflow: hidden;
	text-indent: -9999px;
}

.bbs_page a.next {
	padding-right: 16px;
	background-position: 90% 50%;
}

.bbs_page a:hover,.bbs_page a.next {
	border-color: #09C;
}

.Input_box {
	width: 69px;outline:none;
	border: 1px solid rgb(189, 198, 207);
	margin-left: 5px;margin-right: 3px;
	border-radius: 3px;
	box-shadow: 0 1px 2px rgb(210, 210, 210) inset;
	height: 21px;vertical-align: top;
	transition: border 0.3s,box-shadow 0.3s;-webkit-transition: border 0.3s,box-shadow 0.3s;
}
.Input_box:HOVER {
	width: 69px;outline:none;
	border: 1px solid rgb(82,169,255);
	margin-left: 5px;margin-right: 3px;
	border-radius: 3px;
	box-shadow: 0 0 4px rgb(82,169,255);
	height: 21px;vertical-align: top;
}
.Combox_box {
  width: 76px;height: 25px;cursor:pointer; border: 0px;
  background-image: url("../images/NewVersion/Combox.png");
  background-repeat: no-repeat; padding-left: 3px;
  background-color: transparent;margin-left: 7px;
}
.bulletin_bg {
	background: url(../images/NewVersion/Search.png) no-repeat;
	cursor: pointer;
}

.btn_gb44 {
	margin-top: 5px;
	color:#999999;
	display: block; padding-top: 3px;
	width: 53px;height: 22px;
	line-height: 20px;
	text-decoration: none;
}
/*Custom radio button*/
input[type=radio] {
    display:none;visibility: hidden;
}
.radio-btn {
    display: inline-block;
    margin-left: 8px;vertical-align: sub;
    cursor: pointer;position: relative;
    background-image:url('../../tools/Calculator/images/radio.png');background-repeat:no-repeat;
    width:16px;height:16px; margin-right: 4px;
}
.checkedRadio {
    background-image:url('../../tools/Calculator/images/radio1.png');background-repeat:no-repeat;
    width:16px;height:16px;
}
.radio-btn i {
    width: 10px;
    height: 10px;
    position: absolute;
    left: 4px;
    top: 4px;
}
.checkedRadio .checkedRadio i {
    background-color: #898A8C;
}
/*Custom checkbox*/
 .check-box {
    width: 14px; height: 14px;
    cursor: pointer;
    display: inline-block;
    margin: 2px 7px 0 0;
    position: relative; vertical-align: sub;
    overflow: hidden;background-repeat:no-repeat;
    background-image:url('../../tools/Calculator/images/unselect.png');
}
.checkedBox {
    width: 14px; height: 14px;
    cursor: pointer;
    display: inline-block;
    margin: 2px 7px 0 0;
    position: relative;
    overflow: hidden;background-repeat:no-repeat;
    background-image:url('../../tools/Calculator/images/select.png');
}
.checkedBoxUnDisable {
    width: 14px; height: 14px;
    cursor: pointer;
    display: inline-block;
    margin: 2px 7px 0 0;
    position: relative;
    overflow: hidden;background-repeat:no-repeat;
    background-image:url('../../tools/Calculator/images/unselect_disable.png');
}
.checkedBoxDisable {
    width: 14px; height: 14px;
    cursor: pointer;
    display: inline-block;
    margin: 2px 7px 0 0;
    position: relative;
    overflow: hidden;background-repeat:no-repeat;
    background-image:url('../../tools/Calculator/images/select_disable.png');
}
input[type=checkbox]{
    visibility:hidden;display:none;
}