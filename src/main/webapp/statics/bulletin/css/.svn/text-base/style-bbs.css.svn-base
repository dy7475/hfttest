/**创建主题帖的css**/
.ipt-textfield{outline:none; padding-left: 8px;margin: 0;height: 33px;width: 460px;border: 1px solid #CAD5DF;color: #666666;font-size: 14px;font-weight: bold;text-indent: 2px;line-height: 1.5em;background-color: #fff;font-family: Arial, Helvetica, sans-serif;  border-radius: 3px;}

.inputBox{height: 15px;width: 90px;border: 1px solid #bad7ff;color:#333333;padding-top: 3px;text-indent: 2px;margin: 0px;line-height: 15px;background: #fff;font-family: Arial, Helvetica, sans-serif;}
.ipt-textfield-h140{height: 140px;width: 99.8%;border: 1px solid #bad7ff;padding: 0;line-height: 1.5em;background: #f3f9ff;color: #333;font-family: Arial, Helvetica, sans-serif;margin-bottom: 5px;}
.bbs-content ul{list-style: none;margin: 0;padding: 0;}
.bbs-content li{padding: 0;display: block;margin: 13px 0 0 0;list-style: none;white-space: nowrap;}
.bbs-content h1{font-size: 16px;font-family:'宋体'; color: #666666;font-weight: normal;margin: 4px 0 0 0;padding: 0px;}
.bbs-content h1 .lookCount{
	padding-left: 18px;height: 16px; font-family:'Microsoft Yahei'; font-size: 12px;
  	color: #666666; float: right;
}
.bbs-content h1 .lookCount img{
	vertical-align: sub;margin-right: 6px;
}
.bbs-content h1 .replyCount{	
	padding-left: 16px;height: 14px; font-family:'Microsoft Yahei'; font-size: 12px;
  	color: #666666; float: right;
}
.bbs-content h1 .replyCount img{
	vertical-align: sub;margin-right: 6px;
}
.bbs-content h2{font-size: 12px;display: block;margin-bottom: 12px;padding: 0;font-weight: bold;color: #259cf3;}
.bbs-content h2 .JustLookAtMe{
	background-image: url("../images/NewVersion/JustLookAtMe.png");background-repeat: no-repeat;
	width: 14px;height: 14px; float: right;cursor:pointer;
  	margin-right: 8px;margin-left: 10px;
}
.bbs-content h4,.replyContent{font-family: Arial, Helvetica, sans-serif,"宋体";font-size: 12px;word-break:break-all;display: block;padding: 0px 5px;color: #000;width:580px;font-weight: normal;margin: 0 auto;line-height:21px;}
.bbs-content h4 p,.replyContent p{margin:3px auto;line-height: 21px;}
.replyContent{padding-right:25px;width:580px;}
.bbs-content h6{font-family: Arial, Helvetica, sans-serif;font-size: 12px;line-height: 36px;font-weight: normal;color: #0596f5;border-bottom: 1px solid #DAE2EA;clear: both;text-align: right;margin-bottom: 10px;}
.bbs-content-img{padding: 5px;width:40px;height:40px;background-position: -117px -71px;  margin-top: -15px;}
.border1px{background: #e8f3ff;display: block;padding: 0;margin: auto;}
.blue{color: #666666;cursor: pointer;}
.red{color: #F30;}
.bbs-content h6 a{margin-left: 9px;color: #999999;text-decoration: none;background-image: url('../images/NewVersion/replay.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}
.bbs-content h6 a:hover{  color: #1f96ff;text-decoration: none;background-image: url('../images/NewVersion/replayOver.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}

/*新加 CSS 样式*/
.bbs-content h6 .addTop{color: #999999;text-decoration: none;background-image: url('../images/NewVersion/addTop.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}
.bbs-content h6 .addTop:hover{  color: #1f96ff;text-decoration: none;background-image: url('../images/NewVersion/addTopOver.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}
.bbs-content h6 .refresh{color: #999999;text-decoration: none;background-image: url('../images/NewVersion/refresh.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}
.bbs-content h6 .refresh:hover{  color: #1f96ff;text-decoration: none;background-image: url('../images/NewVersion/refreshOver.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}
.bbs-content h6 .editor{color: #999999;text-decoration: none;background-image: url('../images/NewVersion/editor.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}
.bbs-content h6 .editor:hover{  color: #1f96ff;text-decoration: none;background-image: url('../images/NewVersion/editorOver.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}
.bbs-content h6 .delete{color: #999999;text-decoration: none;background-image: url('../images/NewVersion/delete.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}
.bbs-content h6 .delete:hover{  color: #1f96ff;text-decoration: none;background-image: url('../images/NewVersion/deleteOver.png');background-repeat: no-repeat;padding-left: 17px;background-position-y: 1px;}

/*列表页面底部样式**/
.bottomBar{background:#fff;border:1px solid #BAD7FF;overflow:hidden;margin:0;padding:5px 0 5px 0;}
.searchArea{padding-left:10px;text-align:center;}
.searchArea table{margin:0 auto}
.txtCss{border:#D3D3D3 solid 1px;}
.btnCss{border:#c0c0c0 solid 1px;font-size:12px;color:#333;}
/*分页链接**/
.bbs_page{text-align:center;padding:2px;vertical-align:middle;}
.bbs_page a,.bbs_page strong{padding:0 5px;margin-right:2px;border:1px solid;line-height:20px;overflow:hidden;}
.bbs_page a{border-color:#E6E7E1;background-color:#FFF;color:#09C;}
.bbs_page a:hover{text-decoration:none;}
.bbs_page strong{border-color:#09C;background-color:#09C;color:#FFF;font-weight:700;}
.bbs_page a.prev,.bbs_page a.next{background-repeat:no-repeat;}
.bbs_page a.prev{background-position:30% 50%;padding:0;width:22px;overflow:hidden;text-indent:-9999px;}
.bbs_page a.next{padding-right:16px;background-position:90% 50%;}
.bbs_page a:hover,.bbs_page a.next{border-color:#09C;}
/*分页链接**/
.bbs_page_normal{text-align:center;padding:0;vertical-align:middle;height:16px;line-height:16px;}

.bbs_editor{clear: both;}
.tab-vote{position: relative;background: #e8f3ff;border: 1px solid #6b96c3;width:580px;margin:10px auto;padding:10px 0 5px 0}
.tab-vote table{text-align:left;width:96%;margin:0 auto;table-layout: fixed;border-collapse: collapse;border-spacing: 0}
.tab-vote .vote_title{text-align: left;padding-left:15px;font-weight: bold;}
