.tab-vote .radioBox,.checkBox{width:30px;text-align:center;}
.tab-vote .title{width:200px;text-overflow:ellipsis; word-break:keep-all;overflow:hidden;text-align:left;padding-left:5px;padding-right:20px;font-size:12px;}
.tab-vote .votePer{width:200px;padding:0 5px;}
.tab-vote .voteResult{width:100px;white-space: nowrap;font-size:12px;}
.bbs-vote-h-img{position: relative;padding: 0;margin: 0;top: 3px;}
.bbs-vote-z-img{border: 1px solid #009966;top: 3px;position: relative;margin: 0;padding: 1px;}
