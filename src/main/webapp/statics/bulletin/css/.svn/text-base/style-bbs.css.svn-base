/*Custom radio button*/
input[type=radio] {
    display:none;visibility: hidden;
}
.radio-btn {
    display: inline-block;margin-right: 7px;
    margin-left: 8px;vertical-align: sub;
    cursor: pointer;position: relative;
    background-image:url('../../tools/Calculator/images/radio.png');background-repeat:no-repeat;
    width:16px;height:16px;
}
.checkedRadio {
    background-image:url('../../tools/Calculator/images/radio1.png');background-repeat:no-repeat;
    width:16px;height:16px;
}
.radio-btn i {
    width: 10px;
    height: 10px;
    position: absolute;
    left: 4px;
    top: 4px;
}
.checkedRadio .checkedRadio i {
    background-color: #898A8C;
}
/*Custom checkbox*/
 .check-box {
    width: 61px; height: 28px;
    cursor: pointer; border-radius: 3px;
    display: inline-block;
    margin: 2px 7px 0 0;
    position: relative; vertical-align: sub;
    overflow: hidden;border: 1px solid rgb(189,198,207);
    background: #FFFFFF;
}
.check-box .text{
	margin-left: 6px;margin-top: 2px;
  	float: left;
}
.check-box .divChecked{
	display: none;
}
.checkedBox {
    width: 61px; height: 28px;
    cursor: pointer; border-radius: 3px;
    display: inline-block;
    margin: 0px 5px 0 0;
    position: relative; vertical-align: sub;
    overflow: hidden; border: 2px solid #FB8A10;
    background: #FFFFFF;
}
.checkedBox .divChecked{
	background-image: url("../images/NewVersion/checked.png");background-repeat: no-repeat;
	width: 14px;height: 14px; float: right;
  	margin-top: -12px;display: block;
}
.checkedBox .text{
	margin-left: 5px;margin-top: 3px;
  	float: left;
}
.checkedBoxUnDisable {
    width: 59px; height: 31px;
    cursor: pointer;
    display: inline-block;
    margin: 2px 7px 0 0;
    position: relative;
    overflow: hidden;border: 2px solid #CBCBCB;
  	background-color: rgb(229, 229, 229);
}
.checkedBoxUnDisable .text{
	margin-left: 5px;margin-top: 4px;
  	float: left; color: rgb(186, 186, 186);
}
.checkedBoxUnDisable .divChecked{
	display: none;
}
.checkedBoxDisable {
    width: 59px; height: 31px;
    cursor: pointer;
    display: inline-block;
    margin: 2px 7px 0 0;
    position: relative;
    overflow: hidden;border: 2px solid #CBCBCB;
  	background-color: rgb(229, 229, 229);
}
.checkedBoxDisable .divChecked{
	background-image: url("../images/NewVersion/checked1.png");background-repeat: no-repeat;
	width: 14px;height: 14px; float: right;
  	margin-top: -10px;display: block;
}
.checkedBoxDisable .text{
	margin-left: 5px;margin-top: 4px;
  	float: left; color: rgb(186, 186, 186);
}
