
body::-webkit-scrollbar-button:vertical:decrement{
	background-image: url('../images/scroll/scroll_arrow_up.png');width: 8px;height: 10px;background-repeat:no-repeat;
}
body::-webkit-scrollbar-button:vertical:increment{
	background-image: url('../images/scroll/scroll_arrow_down.png');width: 8px;height: 10px;background-repeat:no-repeat;
}
body::-webkit-scrollbar-button:horizontal:decrement{
	background-image: url('../images/scroll/scroll_arrow_left.png');width: 8px;height: 10px;background-repeat:no-repeat;
}
body::-webkit-scrollbar-button:horizontal:increment{
	background-image: url('../images/scroll/scroll_arrow_right.png');width: 8px;height: 10px;background-repeat:no-repeat;
}