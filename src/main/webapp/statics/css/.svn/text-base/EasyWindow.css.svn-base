
.easy-window{
	background-color:#EFF4F8; z-index: 3;
	position: absolute; box-shadow: 0 0 10px rgb(42, 54, 94);;
	margin-top: 0px;margin-left: 0px; overflow: hidden;
	font-family:'宋体';font-size:14px;
}
.easy-window .div-title{
	border-bottom: 1px solid #D0DAE3;height: 29px;
		padding-top: 10px;	cursor: move;
}
.easy-window .div-title img{
	vertical-align: middle; margin-right: 9px; margin-left: 10px;margin-top: -7px;
}
.easy-window .div-title span{
	font-family: 'Microsoft Yahei';font-size: 13px;color:#567c98;
}
.easy-window .div-title .bt-close{
	float: right;margin-top: -10px;
	transition:background-color 0.2s;
}
.easy-toast{
	border-radius: 10px;background-color: #333;display:table;
}
.easy-toast .toast-bg{
	border: 0;margin: 0;padding: 0;background: black;
	position: absolute;top:0;left: 0;width: 100%;height: 100%;
}
.easy-toast .toast{
	border: 0;margin: 0;padding: 0;
	text-align: center;vertical-align:middle;
	display:table-cell;color: #fff;
}
.easy-window iframe{
	border:0px;width: 100%; height: 100%;
}


