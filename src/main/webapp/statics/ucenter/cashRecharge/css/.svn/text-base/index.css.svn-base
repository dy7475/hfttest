.qr-code-model{display: none;position: absolute;width: 100%;height: 100%;background: rgba(0,0,0,0.4);z-index: 9999;}
.qr-code-model .inner-qr-box{box-sizing: border-box; margin: 55px 0 0 210px;width: 305px;height: 305px;background-color: #fff;}
.qr-code-model .qr-box-close{padding: 5px;cursor:pointer; background: url("../../common/img/close.png") no-repeat center; background-size: 16px 16px;width: 16px;height: 16px;float: right;}
.qr-code-model .qr-box{margin: 40px 0 0 65px;width: 150px;height: 150px;border: 1px solid #26abee;padding: 10px;display: inline-block;}
.qr-code-model .qr-box.weixin{border: 1px solid #42b035;width: 170px;height: 170px;overflow: hidden;padding: 0;}
.qr-code-model .qr-box.weixin img{width: 175px;height: 175px;}
.qr-code-model .qr-box.weixin .weixin-qr-img{width: 170px;height: 170px;}
.qr-code-model .qr-box.weixin .weixin-pay-img{position: absolute;width: 25px;height: 24px;margin: -95px 0 0 75px;background-color: #fff;}
.qr-code-model .qr-box.weixin .weixin-pay-img img{width: 25px;height: 24px;}
.qr-code-model .qr-img{width: 150px;height: 150px;}
.qr-code-model .pay-info-box{text-align: center;margin-top: 10px;}
.qr-code-model .pay-info-box span:nth-child(1){color: #8b8b8b;font-size: 15px;}
.qr-code-model .pay-info-box span:nth-child(2){color: #ff7b05;font-size: 25px;}
.qr-code-model .pay-info-box span:nth-child(3){color: #ff7b05;font-size: 15px;}
.qr-code-model .pay-info-box .info{color: #555;font-size: 15px;margin-top: 5px;}

.success-box{text-align: center;margin-top: 90px;}
.success-box .success-text{font-size: 25px; color: #0ca0ea;margin-top: 10px;}
.success-box .now-balance{font-size: 16px; color: #909090;margin-top: 10px;}
.success-box .now-balance span{color: #ff7801;}
.success-bottom-box{position: absolute;bottom: 30px;left: 250px;}

#custObj{position: absolute;}
