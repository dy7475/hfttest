.alert-model{display: none;position: absolute;width: 100%;height: 100%;background: rgba(0,0,0,0.4);z-index: 9999;}
.alert-model .alert-content{
	left: 50%;width: 330px;height: 185px;
	position: absolute;top:50%;background: #ffffff;
	-webkit-transform: translate(-50%,-50%);
	-moz-transform: translate(-50%,-50%);
	-ms-transform: translate(-50%,-50%);
	-o-transform: translate(-50%,-50%);
	transform: translate(-50%,-50%);
}
