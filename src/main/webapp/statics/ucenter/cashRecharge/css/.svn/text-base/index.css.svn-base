.alert-model .alert-footer {position: relative;width: 100%;height: 40px;background: #fff;}
.alert-model .ok {
	position: absolute;top: 50%;
	left: 135px;width: 64px;
	height: 28px;text-align: center;
	font-size: 12px;color: #ffffff;
	background: #4fb0ff;margin-top: -14px;
	border:1px solid #4fb0ff;
	border-radius: 3px;line-height: 28px;
	cursor: pointer;
}
.alert-model .cancel {
	position: absolute;top: 50%;
	left: 135px;width: 64px;
	height: 28px;text-align: center;
	font-size: 12px;color: #4fb0ff;
	background: #fff;margin-top: -14px;
	border:1px solid #4fb0ff;
	border-radius: 3px;line-height: 28px;
	cursor: pointer;
}

