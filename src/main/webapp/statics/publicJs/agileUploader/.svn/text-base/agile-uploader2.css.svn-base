.agileUploaderFilePreview {position: relative; display: block; height:56px; width: 82px; font-size:12px;overflow: hidden;}
.agileUploaderFilePreview img{height:56px; width: 82px;}
.agileUploaderFilePreview cite.hx {background: #111; filter:alpha(opacity=50); opacity:.5;
	color: red; font-weight:bold; position: absolute; bottom: 0; right: 0;width: 100%;height:25px;line-height:25px;
	border-top: 1px solid #999; text-align:left;
}
.showLabel cite.firstlabel{z-index:100;background:url(firstlabel.gif) no-repeat;position: absolute; bottom: 0; right: 0;width: 40px;height:40px;}
.agileUploaderFileSize {
	text-align: left;
	font-size:12px;
	margin:2px 0;
	overflow:hidden;
}
.agileUploaderFileName {
	text-align:left;
}
#agileUploaderInfo {overflow:hidden;border:1px solid #BAD7FF;padding:10px 0;}
#agileUploaderFileList {
	display:table;
	border-collapse: separate;
}
#agileUploaderFileList li {
	width:82px;
	height:75px;
	margin:5px 16px;
	border: solid 1px #BAD7FF;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow:hidden;
    float:left;
}
#agileUploaderFileList .item_huxing {
    background-color: #f8f8f8;
}
.agileUploaderCurrentProgress {
	height: 6px;
	width: 1%;
	background: #f90;
	margin-top:5px;
}
.setCover{color:#666;}
.setCover:hover{color:#d00;text-decoration: none;}
.agileUploaderRemoveFile {margin-top:5px}
.agileUploaderRemoveFile a{background: url(trash-icon.png) -5px -65px no-repeat;width:11px;height:11px;margin-top:3px;display: block;float:right;}
.only .agileUploaderRemoveFile a{filter:alpha(opacity=50);opacity:.5;outline:none;cursor:default;}
.only .agileUploaderRemoveFile a:hover{background-position: -5px -65px;}
.only .setCover{filter:alpha(opacity=50);opacity:.5;outline:none;cursor:default;text-decoration: none;color:#666;}
.agileUploaderRemoveFile a:hover {background-position: -5px -65px;text-decoration: none;outline:none}
#agileUploaderAttachArea {border-top: 1px solid #DEDEDE;height: 28px;padding: 7px 5px 0;display: none;}
#agileUploaderMessages {
	font-size: 12px;
	color: #FFF;
	font-family: Helvetica, sans-serif;
	padding: 8px 0px 8px 10px;
	text-align: left;
	position: fixed;
	_position: absolute;
	width:100%;
	background: #666; filter:alpha(opacity=70); opacity:.7;
	top:0;
}
#agileUploaderSWF, #agileUploaderEMBED {
	float: left;
}
#agileUploaderRemoveAll {
	display:none;
	float:left;margin-top:8px;
	height: 14px;
	text-align: right;
	width: 100px;
	font-family: Helvetica, sans-serif;
}
#agileUploaderRemoveAll a {
	color: #555;
	font-size: 12px;
}
#agileUploaderRemoveAll a:hover {
	color: #000;
}
.agileUploaderSingleMessages {
	clear: left !important;
	text-align: left !important;
}
#agileUploaderInfoContainer {
	float: left;
	width: 148px;
	margin-right: 5px;
}
#agileUploaderProgressBar {
	clear: left;
	width: 0px;
	height: 2px;
	margin: 0px;
	padding: 0px;
}
#agileUploaderFileInputText {
	width: 100%;
}

.agileUploaderInfo_desc{padding-top:4px;position: relative;}
.filesize{height: 18px;vertical-align: left;display:block;display: none}
object{outline:none}
.fixtop{position: absolute;width:100%;top:-15px;}
.fixbtm{width:100%;}